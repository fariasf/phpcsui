{% extends "@App/layout.html.twig" %}

{% block content %}
    <h1>Test</h1>
    {% for file in files %}
        <h2>{{ file.getRelativeFilename() }}</h2>
        <ul>
        {% for error in file.getErrors() %}
            <li>{{ error.getLine() }}: {{ error.getMessage() }}</li>
        {% endfor %}
        </ul>
        
    {% endfor %}
{% endblock %}
